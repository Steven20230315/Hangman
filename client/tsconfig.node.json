{
	"compilerOptions": {
		"composite": true,
		"skipLibCheck": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"plugins": [
			{
				"name": "@styled/typescript-styled-plugin",
				"tags": ["styled", "css", "sty"]
			}
		]
	},
	"include": ["vite.config.ts"]
}
